<!DOCTYPE html>
<html>
<head>
	<title>Index Page</title>
</head>
	<body>
	
		<nav>
			<a href="/users/new">New Post</a>
		</nav>

		<h1>Users Lists</h1>

		<ul>
		<% @users.each_with_index do |user,index| %>
		  <li>
		  <a href ="/users/<%= index %>"><%= user.first_name %> <%= user.last_name %> <%= user.dob %></a>
		  </li> 
		  <% end %>
		</ul>
	</body>
</html>